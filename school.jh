entity Student {
	firstName String
    lastName String
}
entity SchollClass {
	name String
}
entity Group {
	name String
}
entity GradeLevel {
	value Integer
}
entity Subject {
	name String
    common Boolean
}

relationship ManyToMany {
	//Job{task(title)} to Task{job}
	Group{subjects} to Subject{groups}
    Group{students} to Student{groups}
 	Student{groups} to Group{students}
    SchollClass{gradeLevels} to GradeLevel{schollClasses}
    Subject{gradeLevels} to GradeLevel{subjects}
}

relationship ManyToOne {
  
    Group{gradeLevel} to GradeLevel 
    Student{gradeLevel} to GradeLevel 
	Student{schoolClass} to  SchollClass
}
//relationship OneToMany {
	
//}


// Set pagination options
paginate Article, Novel with infinite-scroll
paginate Author with pagination

